account oniani
from oniani
user onianidavid@gmail.com

host smtp.gmail.com
port 587

auth on
passwordeval "pass email"

tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

logfile /home/oniani/.cache/msmtp/msmtp.log

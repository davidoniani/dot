column -s'|' -t < "${XDG_DATA_HOME:-$HOME/.local/share}/paper.csv" |
    rofi -dmenu -font "JetBrainsMono Nerd Font 16" -i -l 20 -p "Paper" -theme Paper |
    grep -o "\S*https\S*" |
    xargs -r firefox-developer-edition

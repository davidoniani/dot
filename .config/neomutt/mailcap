audio/*; mpv %s >/dev/null 2>&1 &
video/*; mpv %s >/dev/null 2>&1 &
image/*; nsxiv %s >/dev/null 2>&1 &
text/plain; less %s
text/html; brave %s >/dev/null 2>&1 &; nametemplate=%s.html
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -collapse_br_tags -dump %s; nametemplate=%s.html; copiousoutput
application/pdf; zathura %s >/dev/null 2>&1 &
application/msword; lowriter %s >/dev/null 2>&1 &
